{
  "name": "stress-alert",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:seppanenjp/stress-alert.git",
  "author": "Jukka-Pekka Sepp√§nen <jukka-pekka.seppanen@vincit.fi>",
  "license": "MIT",
  "scripts": {
    "dev": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "deploy": "yarn build && gcloud beta app deploy --version prod-1 && yarn deploy-cron",
    "deploy-cron": "gcloud app deploy cron.yaml --quiet",
    "watch": "nodemon",
    "start": "node ./dist/index.js",
    "build": "tsc --build"
  },
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "mongoose": "^6.7.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "engines": {
    "node": "16.17.1"
  }
}
